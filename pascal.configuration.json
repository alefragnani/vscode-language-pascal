{
    "comments": {
        "lineComment": "//",
        "blockComment": [ "{", "}" ]
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["<", ">"],
        ["'", "'"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["<", ">"],
        ["'", "'"]
    ],
    "folding": {
		"markers": {
			"start": "^\\s*\\{\\$REGION(\\s\\'.*\\')?\\}",
			"end": "^\\s*\\{\\$ENDREGION\\}"
		}
	}

}